{
  "name": "telegram-bot-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Telegram bot starter template",
  "imports": {
    "#root/*": "./build/src/*"
  },
  "author": "deptyped <deptyped@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "typecheck": "tsc",
    "build": "tsc --noEmit false",
    "dev": "tsc-watch --onSuccess \"tsx ./src/main.ts\"",
    "start": "tsc && tsx ./src/main.ts",
    "start:force": "tsx ./src/main.ts",
    "prepare": "husky || true",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push --accept-data-loss",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@grammyjs/auto-chat-action": "^0.1.1",
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/hydrate": "^1.4.1",
    "@grammyjs/i18n": "^1.1.2",
    "@grammyjs/menu": "^1.3.0",
    "@grammyjs/parse-mode": "^1.10.0",
    "@grammyjs/storage-prisma": "^2.4.2",
    "@grammyjs/types": "^3.15.0",
    "@hono/node-server": "^1.13.5",
    "@prisma/client": "^6.0.1",
    "callback-data": "^1.1.1",
    "date-fns": "^4.1.0",
    "grammy": "^1.31.1",
    "grammy-guard": "^0.5.0",
    "hono": "^4.6.9",
    "iso-639-1": "^3.1.3",
    "minecraft-protocol": "^1.50.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "temporal-polyfill": "^0.2.5",
    "tsx": "^4.19.2",
    "znv": "^0.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/node": "^22.9.0",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prisma": "^6.0.1",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
